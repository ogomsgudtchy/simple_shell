#include "main.h"
#include <unistd.h>
#include <stdio.h>

/**
 * calcsum -  calculates a sum of 2 numbers
 * @num1: number entered
 * @num2: number entered
 *
 * Description -
 * Return: 0
 */
int calcsum(int num1, int num2)
{
int sum = 0;
sum = num1 + num2;
printf("the sum is %d:", sum);
return (0);
}
